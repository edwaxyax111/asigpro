<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Empleado" table="empleado">
    <id name="idEmpleado" type="integer" column="id_empleado">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaNacimiento" type="date" column="fecha_nacimiento" nullable="false"/>
    <field name="profesion" type="string" column="profesion" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="120" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userName" type="string" column="user_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pass" type="string" column="pass" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
